[
  {
    "id": "pixel_to_world_2d",
    "name": "Pixel to World 2D Transform",
    "description": "Transform from pixel coordinates to physical world coordinates",
    "input_space": {
      "id": "pixel_space", 
      "name": "Pixel Space",
      "dimensions": [
        {"name": "x", "unit": "index", "type": "space"},
        {"name": "y", "unit": "index", "type": "space"}
      ]
    },
    "output_space": {
      "id": "world_space_2d",
      "name": "World Space 2D", 
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"}
      ]
    },
    "transform": {
      "type": "homogeneous",
      "matrix": [[0.5, 0, 100], [0, 0.5, 200], [0, 0, 1]]
    }
  },
  {
    "id": "world_registration",
    "name": "World Space Registration",
    "description": "Align world coordinates between different samples",
    "input_space": {
      "id": "world_space_sample1",
      "name": "World Space Sample 1",
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"},
        {"name": "z", "unit": "micrometer", "type": "space"}
      ]
    },
    "output_space": {
      "id": "world_space_reference",
      "name": "Reference World Space",
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"},
        {"name": "z", "unit": "micrometer", "type": "space"}
      ]
    },
    "transform": {
      "type": "sequence",
      "sequence": [
        {"type": "translation", "translation": [10, 20, 5]},
        {"type": "rotation", "matrix": [[0.866, -0.5, 0], [0.5, 0.866, 0], [0, 0, 1]]}
      ]
    }
  },
  {
    "id": "spherical_to_cartesian",
    "name": "Spherical to Cartesian Transform",
    "description": "Convert spherical coordinates to Cartesian world space",
    "input_space": {
      "id": "spherical_space",
      "name": "Spherical Coordinate Space",
      "dimensions": [
        {"name": "r", "unit": "micrometer", "type": "space"},
        {"name": "theta", "unit": "radian", "type": "space"},
        {"name": "phi", "unit": "radian", "type": "space"}
      ]
    },
    "output_space": {
      "id": "cartesian_space",
      "name": "Cartesian World Space",
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"},
        {"name": "z", "unit": "micrometer", "type": "space"}
      ]
    },
    "transform": {
      "type": "coordinates",
      "coordinates": {
        "conversion": "spherical_to_cartesian",
        "formulas": {
          "x": "r * sin(phi) * cos(theta)",
          "y": "r * sin(phi) * sin(theta)", 
          "z": "r * cos(phi)"
        }
      }
    }
  },
  {
    "id": "time_series_alignment",
    "name": "Time Series Alignment",
    "description": "Temporal alignment of time series data",
    "input_space": {
      "id": "raw_time_series",
      "name": "Raw Time Series",
      "dimensions": [
        {"name": "t", "unit": "second", "type": "time"}
      ]
    },
    "output_space": {
      "id": "aligned_time_series", 
      "name": "Aligned Time Series",
      "dimensions": [
        {"name": "t", "unit": "second", "type": "time"}
      ]
    },
    "transform": {
      "type": "translation",
      "translation": [-2.5]
    }
  },
  {
    "id": "multichannel_projection",
    "name": "Multichannel Projection",
    "description": "Project multichannel data to RGB space",
    "input_space": {
      "id": "multichannel_fluorescence",
      "name": "Multichannel Fluorescence Space",
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"},
        {"name": "z", "unit": "micrometer", "type": "space"},
        {"name": "channels", "unit": "index", "type": "other"}
      ]
    },
    "output_space": {
      "id": "rgb_space",
      "name": "RGB Display Space",
      "dimensions": [
        {"name": "x", "unit": "micrometer", "type": "space"},
        {"name": "y", "unit": "micrometer", "type": "space"},
        {"name": "z", "unit": "micrometer", "type": "space"},
        {"name": "rgb", "unit": "index", "type": "other"}
      ]
    },
    "transform": {
      "type": "mapAxis",
      "mapAxis": {
        "channels[0]": "rgb[0]",
        "channels[1]": "rgb[1]", 
        "channels[2]": "rgb[2]"
      }
    }
  }
]