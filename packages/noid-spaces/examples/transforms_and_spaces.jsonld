{
  "@context": {
    "@base": "https://example.org/",
    "noid_spaces": "https://github.com/nclack/noid/schemas/space/",
    "noid_transform": "https://github.com/nclack/noid/schemas/transform/",
    "id": "@id"
  },
  "@graph": [
    {
      "@type": "noid_spaces:CoordinateSystem",
      "id": "image_space",
      "noid_spaces:dimensions": [
        "image_space#x",
        "image_space#y",
        "image_space#z"
      ],
      "noid_spaces:description": "Image coordinate system in millimeters"
    },
    {
      "@type": "noid_spaces:CoordinateSystem",
      "id": "world_space",
      "noid_spaces:dimensions": [
        "world_space#x",
        "world_space#y",
        "world_space#z"
      ],
      "noid_spaces:description": "World coordinate system in millimeters"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "image_space#x",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "image_space#y",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "image_space#z",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "world_space#x",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "world_space#y",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "world_space#z",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "transform_with_references",
      "noid_spaces:input": "image_space",
      "noid_spaces:output": "world_space",
      "noid_transform:transform": {
        "noid_transform:translation": [10.0, 20.0, 0.0]
      }
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "transform_with_inline_systems",
      "noid_spaces:input": {
        "id": "input_space",
        "noid_spaces:dimensions": [
          "input_space#x",
          "input_space#y"
        ]
      },
      "noid_spaces:output": {
        "id": "output_space",
        "noid_spaces:dimensions": [
          "output_space#x",
          "output_space#y"
        ]
      },
      "noid_transform:transform": {
        "@type": "noid_transform:Scale",
        "noid_transform:scale": [1000.0, 1.0]
      }
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "input_space#x",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "input_space#y",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "output_space#x",
      "noid_spaces:unit": "µm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "output_space#y",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "pixel_to_world_transform",
      "noid_spaces:input": {
        "id": "pixel_space",
        "noid_spaces:dimensions": [
          "pixel_space#x"
        ]
      },
      "noid_spaces:output": {
        "id": "world_1d_space",
        "noid_spaces:dimensions": [
          "world_1d_space#x"
        ]
      },
      "noid_transform:transform": {
        "noid_transform:translation": [5.0]
      },
      "noid_spaces:description": "Transform from pixels to world coordinates"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "pixel_space#x",
      "noid_spaces:unit": "pixel",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "world_1d_space#x",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "simple_index_to_spatial",
      "noid_spaces:input": {
        "noid_spaces:dimensions": [
          {
            "id": "i",
            "noid_spaces:unit": "index",
            "noid_spaces:type": "index"
          },
          {
            "id": "j",
            "noid_spaces:unit": "index",
            "noid_spaces:type": "index"
          }
        ]
      },
      "noid_spaces:output": {
        "noid_spaces:dimensions": [
          {
            "id": "x",
            "noid_spaces:unit": "mm",
            "noid_spaces:type": "space"
          },
          {
            "id": "y",
            "noid_spaces:unit": "mm",
            "noid_spaces:type": "space"
          }
        ]
      },
      "noid_transform:transform": {
        "noid_transform:homogeneous": [
          [0.1, 0.0, 5.0],
          [0.0, 0.1, 10.0],
          [0.0, 0.0, 1.0]
        ]
      },
      "noid_spaces:description": "Simple array indices to spatial coordinates"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "noid_spaces:input": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "index",
            "noid_spaces:type": "index"
          }
        ]
      },
      "noid_spaces:output": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "s",
            "noid_spaces:type": "time"
          }
        ]
      },
      "noid_transform:transform": {
        "@type": "noid_transform:Scale",
        "noid_transform:scale": [0.1]
      },
      "noid_spaces:description": "Time series: frame index to temporal coordinates (10 FPS)"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "axis_permutation_transform",
      "noid_spaces:input": {
        "id": "yxz_space",
        "noid_spaces:dimensions": [
          "yxz_space#AP",
          "yxz_space#ML",
          "yxz_space#DV"
        ]
      },
      "noid_spaces:output": {
        "id": "xyz_space",
        "noid_spaces:dimensions": [
          "xyz_space#ML",
          "xyz_space#AP",
          "xyz_space#DV"
        ]
      },
      "noid_transform:transform": {
        "noid_transform:map_axis": [1, 0, 2]
      },
      "noid_spaces:description": "Axis permutation: AP-ML-DV → ML-AP-DV reordering"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "yxz_space#AP",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "yxz_space#ML",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "yxz_space#DV",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "xyz_space#ML",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "xyz_space#AP",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "xyz_space#DV",
      "noid_spaces:unit": "mm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "noid_spaces:input": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "index"
          }
        ]
      },
      "noid_spaces:output": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "nm",
            "noid_spaces:type": "other"
          }
        ]
      },
      "noid_transform:transform": {
        "noid_transform:translation": [400.0]
      },
      "noid_spaces:description": "Spectral mapping: channel index to wavelength (starting at 400nm)"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "noid_spaces:input": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "index"
          }
        ]
      },
      "noid_spaces:output": {
        "noid_spaces:dimensions": [
          {
            "noid_spaces:unit": "mm"
          }
        ]
      },
      "noid_transform:transform": {
        "noid_transform:displacements": {
          "noid_sampler:path": "/path/to/irregular_positions.zarr",
          "noid_sampler:interpolation": "linear",
          "noid_sampler:extrapolation": "nearest"
        }
      },
      "noid_spaces:description": "Lookup table: irregular grid positions from external file"
    },
    {
      "@type": "noid_spaces:CoordinateTransform",
      "id": "mm_to_micron_transform",
      "noid_spaces:input": "world_space",
      "noid_spaces:output": {
        "id": "micron_space",
        "noid_spaces:dimensions": [
          "micron_space#x",
          "micron_space#y",
          "micron_space#z"
        ]
      },
      "noid_transform:transform": {
        "noid_transform:scale": [1000.0, 1000.0, 1000.0]
      },
      "noid_spaces:description": "Unit conversion: mm to µm scaling"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "micron_space#x",
      "noid_spaces:unit": "µm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "micron_space#y",
      "noid_spaces:unit": "µm",
      "noid_spaces:type": "space"
    },
    {
      "@type": "noid_spaces:Dimension",
      "id": "micron_space#z",
      "noid_spaces:unit": "µm",
      "noid_spaces:type": "space"
    }
  ]
}
